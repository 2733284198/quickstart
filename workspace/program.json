{
    "boot": "github.com/firephp/quickstart/",
    "engine": [
        "node"
    ],
    "extends": {
        "location": "./program.packages.json"
    },
    "packages": {
        "github.com/firephp/quickstart/": {
            "locator": {
                "location": "./"
            }
        },
        "nodejs.org": {
            "provider": "nodejs.org"
        },
        "pinf.org/loader": {
            "provider": "pinf.org/loader"
        },
        "github.com/pinf/modules-js/": {
            "locator": {
                "archive": "https://github.com/pinf/modules-js/zipball/v0.1.2"
            }
        },
        "github.com/kriskowal/q/": {
            "locator": {
                "archive": "https://github.com/kriskowal/q/zipball/v0.3.0"
            },
            "descriptor": {
                "uid": "https://github.com/kriskowal/q/",
                "dependencies": [
                    {
                        "id": "github.com/pinf/modules-js/"
                    }
                ]
            }
        }
    }
}